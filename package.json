{
  "name": "demo-examples",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite-serve": "npx vite preview",
    "vite-build": "node_modules/typescript/bin/tsc && npx vite build",
    "parcel-serve": "npx parcel index.html",
    "parcel-build": "node_modules/typescript/bin/tsc && npx parcel build index.html --public-url .",
    "webpack-serve": "mkdir -p webpack-assets && sed \"s/\\/src\\/main\\.ts/\\.\\/main\\.js/\" index.html > ./webpack-assets/index.html && npx webpack serve ./src/main.ts --static webpack-assets --mode development",
    "webpack-build": "node_modules/typescript/bin/tsc && npx webpack ./src/main.ts && sed \"s/\\/src\\/main\\.ts/\\.\\/main\\.js/\" index.html > ./dist/index.html"
  },
  "dependencies": {
    "scenerystack": "~0.0.5",
    "jquery": "~2.1.0"
  },
  "devDependencies": {
    "vite": "^5.4.10",
    "parcel": "^2.12.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",

    "eslint": "~8.28.0",
    "typescript": "~5.3.3",
    "@types/jquery": "~3.5.13",

    "@typescript-eslint/parser": "~6.18.1",
    "@typescript-eslint/eslint-plugin": "~6.18.1",
    "@typescript-eslint/utils": "~6.18.1",
    "@types/lodash": "~4.14.172",
    "@types/p2": "~0.7.39",
    "@types/pako": "~2.0.0",
    "@types/qunit": "~2.19.6",
    "@types/three": "~0.137.0",
    "@webgpu/types": "~0.1.40"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}